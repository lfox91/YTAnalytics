extends layout

block content
  div(class="row video")
    iframe(width="560" height="315" src=src frameborder="0" allowfullscreen)    
      
  section(class="row")
    div(class="input-field col s10 offset-s1 m8 offset-m2 l6 offset-l3  grey-text text-lighten-3 z-depth-2")
      select(id="select")
        option(value="views" selected) Views
        option(value="likes") Likes
        option(value="dislikes") Dislikes
        option(value="comments") Comments
        option(value="shares") shares
        option(value="estimatedMinutesWatched") Watch Time
        option(value="averageViewDuration") Avg. View Duration
    
  div(class="graphContainer")
    canvas(id="gCanvas")

